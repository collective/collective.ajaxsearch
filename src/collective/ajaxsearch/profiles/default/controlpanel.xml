<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.ajaxseach"
    purge="False">

    <configlet
        title="Collective Ajaxsearch"
        action_id="ajaxsearch"
        appId="collective.ajaxsearch"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@ajaxsearch-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>